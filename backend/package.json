{
  "name": "isdicoders",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/src/framework/Main.js",
    "test": "jest --coverage",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "compile-watch": "tsc -w",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "docker-build": "docker build -t isdicoders/bk-todo:v1 ."
  },
  "jest": {
    "setupFiles": [
      "<rootDir>config.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript"
      }
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ]
  },
  "author": "calbertts",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/node": "^14.11.2",
    "gts": "^3.1.0",
    "jest": "^28.1.3",
    "jest-ts-auto-mock": "^2.1.0",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^28.0.6",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0"
  }
}
